{
  "name": "gfeed-web",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "start": "sst bind vite dev",
    "prepare": "svelte-kit sync",
    "build": "sst bind vite build",
    "preview": "vite preview",
    "test": "bun run test:integration && bun run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@playwright/test": "^1.38.1",
    "@sveltejs/adapter-auto": "^3.2.0",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/svelte": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.34.0",
    "jsdom": "^24.1.0",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "sst": "^2.43.3",
    "svelte-check": "^3.6.9",
    "tslib": "^2.6.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "unplugin-turbo-console": "^1.8.9",
    "vitest": "^2.0.1"
  },
  "type": "module",
  "dependencies": {
    "@kinde-oss/kinde-auth-sveltekit": "^1.3.0",
    "@sentry/sveltekit": "^8.7.0",
    "@sveltejs/kit": "^2.5.6",
    "@tailwindcss/typography": "^0.5.13",
    "autoprefixer": "^10.4.16",
    "crypto": "^1.0.1",
    "daisyui": "^4.12.10",
    "date-fns": "^3.6.0",
    "jose": "^5.2.4",
    "lucide-svelte": "^0.407.0",
    "postcss": "^8.4.31",
    "svelte": "^4.2.18",
    "svelte-french-toast": "^1.2.0",
    "svelte-kit-sst": "^2.41.4",
    "tailwindcss": "^3.4.4",
    "vite": "^5.3.3"
  }
}
