{
  "name": "gfeed-web",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "start": "sst bind vite dev",
    "prepare": "svelte-kit sync",
    "build": "sst bind vite build",
    "preview": "vite preview",
    "test": "bun run test:integration && bun run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@playwright/test": "^1.45.1",
    "@sveltejs/adapter-auto": "^3.2.2",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/svelte": "^5.2.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^2.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.41.0",
    "jsdom": "^24.1.0",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.5",
    "sst": "^2.43.3",
    "svelte-check": "^3.8.4",
    "tslib": "^2.6.3",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "unplugin-turbo-console": "^1.9.2",
    "vitest": "^2.0.1"
  },
  "type": "module",
  "dependencies": {
    "@kinde-oss/kinde-auth-sveltekit": "^1.3.0",
    "@sentry/sveltekit": "^8.17.0",
    "@sveltejs/kit": "^2.5.18",
    "@tailwindcss/typography": "^0.5.13",
    "autoprefixer": "^10.4.19",
    "crypto": "^1.0.1",
    "daisyui": "^4.12.10",
    "date-fns": "^3.6.0",
    "jose": "^5.6.3",
    "lucide-svelte": "^0.407.0",
    "postcss": "^8.4.39",
    "svelte": "^4.2.18",
    "svelte-french-toast": "^1.2.0",
    "svelte-kit-sst": "^2.43.3",
    "tailwindcss": "^3.4.4",
    "vite": "5.0.3"
  }
}
